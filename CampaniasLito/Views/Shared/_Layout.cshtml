<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <meta http-equiv="Expires" content="0" />
    <meta http-equiv="Pragma" content="no-cache" />

    <script type="text/javascript">
        {
            if (history.forward(1))
                location.replace(history.forward(1))
        }

        function nobackbutton() {
            window.location.hash = "Litoprocess";
            window.location.hash = "Again-No-back-button" //chrome
            window.onhashchange = function () { window.location.hash = "Litoprocess"; }
        }

    </script>

    <link href="~/Content/css/Compañias/KFC.css" rel="stylesheet" />
    <link rel="icon" href="~/Content/Logos/KFC-logo.png" sizes="32x32" />

    @Styles.Render("~/bundles/jquery")
    @Styles.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/bundles/js")
    @Styles.Render("~/bundles/css")


    <link href="~/Content/vendor/DataTables/datatables.min.css" rel="stylesheet" />
    <link href="~/Content/vendor/DataTables/DataTables-1.10.20/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/Content/vendor/DataTables/DataTables-1.10.20/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Content/vendor/jquery.mCustomScrollbar.min.css" rel="stylesheet" />

    <link href="~/Content/css/menuresponsive.css" rel="stylesheet" />
    <link href="~/Content/icon/style.css" rel="stylesheet" />
    <link href="~/Content/css/Estilos.css" rel="stylesheet" />
    <link href="~/Content/css/site.css" rel="stylesheet" />
    <link href="~/Content/css/rotating-card.css" rel="stylesheet" />
    <link href="~/Content/css/flexboxgrid.min.css" rel="stylesheet" />
    <link href="~/Content/themes/base/tooltip.css" rel="stylesheet" />
    @*<link href="https://fonts.googleapis.com/css?family=Actor" rel="stylesheet">*@
    <link href="~/Content/sweetalert.css" rel="stylesheet" />
    <link href="~/Content/css/jquery.datetimepicker.css" rel="stylesheet" />
    <link href="~/Content/css/Vistas.css" rel="stylesheet" />
    <link href="~/Content/css/Generales.css" rel="stylesheet" />
    @*<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">*@

    <link href="~/Content/vendor/font-awesome/css/all.css" rel="stylesheet" />


    <style type="text/css">
        .swal-container {
            z-index: 2000;
        }
    </style>

</head>
<body style="font-family: 'Hero'">
    <input type="checkbox" id="btn-menu" onchange="mostrarMenu()" />
    <header>
        <table>
            <thead>
                <tr>
                    <th class="col-xs-1" style="border-radius: 0">
                        <div class="col-xs-12">
                            <div id="logo">
                                <a href="@Url.Action("Index", "Home")" class="inicio"
                                   data-toggle="myToolTip" data-placement="bottom" title="KFC"
                                   onclick="viewPage()">
                                </a>
                            </div>
                            <div id="logo2" style="height: 0">
                                <label for="btn-menu">
                                    <span class="spanhtml" data-toggle="myToolTip" data-placement="bottom" title="Menú"
                                          style="text-decoration: none; list-style: none; color: white">
                                        <img class="txt" src="~/Content/Fotos/icono-menu.png" style="margin-top: -5px; border-radius: 5px; width: 40px; height: 45px" />
                                    </span>
                                </label>
                            </div>
                        </div>
                    </th>
                    <th class="col-xs-9" style="border-radius: 0">

                        <nav class="col-xs-12 menuNav" id="menuNav">
                            <ul class="col-xs-12 ulNav">
                                @if (User.IsInRole("SuperAdmin"))
                                {
                                    <li class="col-xs-8"></li>
                                    <li class="col-xs-1">
                                        <div>
                                            <a id="homeB" href="@Url.Action("Index", "Home")" class="fa fa-home iconomenu @Session["homeB"].ToString()" data-toggle="myToolTip" data-placement="bottom" title="Inicio" onclick="viewPage()"></a>
                                        </div>
                                    </li>
                                    <li class="col-xs-1">
                                        <div>
                                            <a id="rolesB" href="@Url.Action("Index", "Roles")" class="fa fa-user-lock iconomenu @Session["rolesB"].ToString()" data-toggle="myToolTip" data-placement="bottom" title="Roles" onclick="viewPage()"></a>
                                        </div>
                                    </li>
                                    <li class="col-xs-1">
                                        <div>
                                            <a id="compañiasB" href="@Url.Action("Index", "Compañias")" class="fa fa-building iconomenu @Session["compañiasB"].ToString()" data-toggle="myToolTip" data-placement="bottom" title="Compañias" onclick="viewPage()"></a>
                                        </div>
                                    </li>
                                    <li class="col-xs-1 ">
                                        <div>
                                            <a id=" usuariosB" href="@Url.Action("Index", "Usuarios")" class="fa fa-users iconomenu @Session["usuariosB"].ToString()" data-toggle="myToolTip" data-placement="bottom" title="Usuarios" onclick="viewPage()"></a>
                                        </div>
                                    </li>
                                }

                                else
                                {
                                    <li class="col-xs-5"></li>
                                    <li class="col-xs-1 ">
                                        <div>
                                            <a id="homeB" href="@Url.Action("Index", "Home")" class="fa fa-home iconomenu @Session["homeB"].ToString()" data-toggle="myToolTip" data-placement="bottom" title="Inicio" onclick="viewPage()"></a>
                                        </div>
                                    </li>
                                    <li class="col-xs-1 ">
                                        <div>
                                            <a id="regionesB" href="@Url.Action("Index", "Regiones")" class="fa fa-map-marked iconomenu @Session["regionesB"].ToString()" data-toggle="myToolTip" data-placement="bottom" title="Regiones" onclick="viewPage()"></a>
                                        </div>
                                    </li>
                                    <li class="col-xs-1 ">
                                        <div>
                                            <a id="ciudadesB" href="@Url.Action("Index", "Ciudades")" class="fa fa-map-marker-alt iconomenu @Session["ciudadesB"].ToString()" data-toggle="myToolTip" data-placement="bottom" title="Ciudades" onclick="viewPage()"></a>
                                        </div>
                                    </li>
                                    <li class="col-xs-1 ">
                                        <div>
                                            <a id="restaurantesB" href="@Url.Action("Equity", "Restaurantes")" class="fa fa-store iconomenu @Session["restaurantesB"].ToString()" data-toggle="myToolTip" data-placement="bottom" title="Restaurantes" onclick="viewPage()"></a>
                                        </div>
                                    </li>
                                    <li class="col-xs-1 ">
                                        <div>
                                            <a id="familiasB" href="@Url.Action("Index", "Familias")" class="fa fa-boxes iconomenu @Session["familiasB"].ToString()" data-toggle="myToolTip" data-placement="bottom" title="Familias" onclick="viewPage()"></a>
                                        </div>
                                    </li>
                                    <li class="col-xs-1 ">
                                        <div>
                                            <a id="materialesB" href="@Url.Action("Equity", "Materiales")" class="fa fa-barcode iconomenu @Session["materialesB"].ToString()" data-toggle="myToolTip" data-placement="bottom" title="Materiales" onclick="viewPage()"></a>
                                        </div>
                                    </li>
                                    <li class="col-xs-1 ">
                                        <div>
                                            <a id="campañasB" href="@Url.Action("Index", "Campañas")" class="fa fa-calendar-alt iconomenu @Session["campañasB"].ToString()" data-toggle="myToolTip" data-placement="bottom" title="Campañas" onclick="viewPage()"></a>
                                        </div>
                                    </li>
                                }

                            </ul>
                        </nav>
                    </th>
                    <th class="col-xs-1" style="border-radius: 0">
                        <div style="margin-right: -30px; text-align: right">

                            <i class="fas fa-user fa-2x img-circle" data-toggle="myToolTip" data-placement="bottom" title="@User.Identity.Name" style="background-color: #fff; color: #000; padding: 10px">
                            </i>

                            @*@if (Session["Foto"] != null && Session["NombreCompleto"] != null)
                                {
                                    <i class="fas fa-user fa-2x img-circle" data-toggle="myToolTip" data-placement="bottom" title="@Session["NombreCompleto"].ToString()" style="background-color: #fff; color: #000; padding: 10px">
                                    </i>
                                }
                                else if (Session["Foto"] == null && Session["NombreCompleto"] != null)
                                {
                                    <i class="fas fa-user fa-2x img-circle" data-toggle="myToolTip" data-placement="bottom" title="@Session["NombreCompleto"].ToString()" style="background-color: #fff; color: #000; padding: 10px">
                                    </i>
                                }*@
                        </div>
                    </th>
                    <th class="col-xs-1" align="right" style="border-radius: 0">
                        <div class="col-xs-12 margentopsesion2" style="margin-left: 10px">
                            @if (Request.IsAuthenticated)
                            {
                                using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm" }))
                                {
                                    @Html.AntiForgeryToken()
                                    <ul>
                                        <li style="list-style: none; text-align: right">
                                            <a href="javascript:document.getElementById('logoutForm').submit()"
                                               class="lnr lnr-power-switch actionlinkclose txt"
                                               data-toggle="myToolTip" data-placement="bottom" title="Salir" onclick="viewPage()"></a>
                                        </li>
                                    </ul>
                                }
                            }
                            else
                            {
                                <ul>
                                    <li style="list-style: none; text-align: right; margin-left: 40px">
                                        @Html.ActionLink(" ", "Login", "Account", routeValues: null, htmlAttributes: new { id = "loginLink", @class = "actionlinkstart lnr lnr-power-switch txt", style = "list-style:none;" })
                                    </li>
                                </ul>
                            }
                        </div>
                    </th>
                </tr>
            </thead>
        </table>
    </header>

    <main>
        <div class="page-wrapper" style="position: absolute; background-size: cover">
            <div class="py-3">
                <div class="container-fluid" style="height: 10%">
                    <div id="contenedor_carga">
                        <div id="carga">
                        </div>
                        @*<div id="inTurnBlurringTextG">
                                <div id="inTurnBlurringTextG_1" class="inTurnBlurringTextG">K</div>
                                <div id="inTurnBlurringTextG_2" class="inTurnBlurringTextG">F</div>
                                <div id="inTurnBlurringTextG_3" class="inTurnBlurringTextG">C</div>
                                <div id="inTurnBlurringTextG_4" class="inTurnBlurringTextG">.</div>
                                <div id="inTurnBlurringTextG_5" class="inTurnBlurringTextG">.</div>
                                <div id="inTurnBlurringTextG_6" class="inTurnBlurringTextG">.</div>
                            </div>*@
                    </div>
                    @RenderBody()
                </div>
            </div>
        </div>
    </main>


    @Scripts.Render("~/bundles/js")
    @RenderSection("scripts", required: false)

    <script src="~/Content/vendor/DataTables/datatables.min.js"></script>
    <script src="~/Content/vendor/DataTables/DataTables-1.10.20/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/Content/vendor/jquery.mCustomScrollbar.concat.min.js"></script>

    @*<script type="text/javascript" src="https://www.google.com/jsapi"></script>*@
    <script src="~/Scripts/fileupload.js"></script>
    <script src="~/Content/js/site.js"></script>
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>*@
    <script src="~/Content/vendor/font-awesome/js/fontawesome.js"></script>

    @Scripts.Render("~/bundles/jqueryval")

    <script>

        $(document).ready(function () {

            $('[data-toggle="myToolTip"]').tooltip();

            $(".tip-top").tooltip({
                placement: 'top'
            });
            $(".tip-right").tooltip({
                placement: 'right'
            });
            $(".tip-bottom").tooltip({
                placement: 'bottom'
            });
            $(".tip-left").tooltip({
                placement: 'left',
                html: true
            });

            $(".tip-auto").tooltip({
                placement: 'auto',
                html: true
            });

        });

    </script>
    <script>

        function mostrarMenu(obj) {
            if (document.getElementById("btn-menu").checked)
                document.getElementById('menuNav').style.marginTop = '40px';

            else
                document.getElementById('menuNav').style.marginTop = '-180%';
        }



        window.onload = function () {
            var contenedor = document.getElementById('contenedor_carga');
            contenedor.style.visibility = 'hidden';
            contenedor.style.opacity = '0';

            //swal({
            //    title: "<span style='color:#fff; margin-top: 5%'></span>",
            //    html: true,
            //    showConfirmButton: false,
            //    animation: true,
            //    customClass: 'animated swing',
            //    background: '#444',
            //    grow: 'fullscreen',
            //    timer: 1000,
            //});

        }

    </script>

    <script>
        $(document).ready(function () {
            $('#tablaRestaurantes').DataTable({
                language: {
                    "sProcessing": "Procesando...",
                    "sLengthMenu": "Mostrar _MENU_ registros",
                    "sZeroRecords": "No se encontraron resultados",
                    "sEmptyTable": "Ningún dato disponible en esta tabla =(",
                    "sInfo": "Mostrando registros del _START_ al _END_ de un total de _TOTAL_ registros",
                    "sInfoEmpty": "Mostrando registros del 0 al 0 de un total de 0 registros",
                    "sInfoFiltered": "(filtrado de un total de _MAX_ registros)",
                    "sInfoPostFix": "",
                    "sSearch": "Buscar:",
                    "sUrl": "",
                    "sInfoThousands": ",",
                    "sLoadingRecords": "Cargando...",
                    "oPaginate": {
                        "sFirst": "Primero",
                        "sLast": "Último",
                        "sNext": "Siguiente",
                        "sPrevious": "Anterior"
                    },
                    "oAria": {
                        "sSortAscending": ": Activar para ordenar la columna de manera ascendente",
                        "sSortDescending": ": Activar para ordenar la columna de manera descendente"
                    },
                    "buttons": {
                        "copy": "Copiar",
                        "colvis": "Visibilidad"
                    }
                },
                dom: 'lfBrtip',
                autoWidth: true,
                //responsive: true,
                buttons: [{
                    extend: 'excel',
                    text: '<i class="fas fa-file-excel"></i>',
                    titleAtrr: 'Exportar a Excel',
                    className: 'btn btn-success',
                    exportOptions: {
                        columns: ":visible",
                    },
                    body: true,
                    footer: true,
                }],
                fixedColumns: true,
                scrollY: 360,
                scrollX: true,
                keys: true,
                'bLengthChange': true,
                'bInfo': true,
                "lengthMenu": [[15, 25, 50, 100, -1], [15, 25, 50, 100, "TODOS"]],
            });
        });


    </script>

    @if (TempData["mensajeLito"] != null)
    {
        <script type="text/javascript">

            swal({
                title: "<span style='color:#fff'>@ViewBag.Title</span>",
                text: '<span style="color:#fff">@TempData["mensajeLito"].ToString()</span>',
                html: true,
                type: "success",
                showConfirmButton: false,
                animation: true,
                customClass: 'animated swing',
                background: '#B3C9D8',
                grow: 'fullscreen',
                timer: 2000,
            });
        </script>
    }

</body>
</html>
